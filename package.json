{
  "name": "@example/minimal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env TELEMETRY_DISABLED=1 ASTRO_TELEMETRY_DISABLED=1 NODE_ENV=dev astro dev",
